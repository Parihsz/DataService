{
    "name": "dataservice-v2",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Shared": {
                "$path": "src/Shared",
                "$ignoreUnknownInstances": true
            },
            "Client": {
                "$path": "src/Client",
                "$ignoreUnknownInstances": true
            }   
        },
        "ServerScriptService": {
            "$path": "src/Server",
            "$ignoreUnknownInstances": true
        }
    }
}